import net.minecraft.creativetab.CreativeTabs;

public class ModTabs {
	
	private static final ArmourTab tabarmour = new ArmourTab("tabArmour");
	private static final ToolsTab tabtools = new ToolsTab("tabTools");
	
	public static CreativeTabs getArmorTab() {
		return tabarmour;
	}
}
