package com.cookiehook.armourexpansion.item;

import com.cookiehook.armourexpansion.init.ModTabs;

import net.minecraft.item.ItemAxe;

public class ItemModAxe extends ItemAxe
{
	public final ToolMaterial toolMaterial;
	
	public ItemModAxe(ToolMaterial toolMaterial, String name)
	{
		super(toolMaterial);
		this.toolMaterial = toolMaterial;
		setUnlocalizedName(name);
		setTextureName("armorexpansion:" + name);
		setCreativeTab(ModTabs.getToolsTab());
	}
}